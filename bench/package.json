{
  "name": "@schema-benchmarks/bench",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./bench.json": "./bench.json",
    "./download.json": "./download.json",
    "./stack.json": "./stack.json"
  },
  "scripts": {
    "bench": "node ./src/scripts/bench/orchestrate.ts",
    "download": "node ./src/scripts/download.ts",
    "stack": "node --expose-gc --max-old-space-size=8192 ./src/scripts/stack.ts",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "@ryoppippi/unplugin-typia": "catalog:",
    "@schema-benchmarks/schemas": "workspace:*",
    "@schema-benchmarks/utils": "workspace:*",
    "@standard-schema/spec": "catalog:",
    "gzip-size": "^7.0.0",
    "rolldown": "1.0.0-rc.5",
    "tinybench": "^6.0.0",
    "typescript": "catalog:",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:"
  }
}
